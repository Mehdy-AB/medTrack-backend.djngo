{
    "datacenter": "dc1",
    "data_dir": "/consul/data",
    "log_level": "INFO",
    "node_name": "consul-server",
    "server": true,
    "bootstrap_expect": 1,
    "ui_config": {
        "enabled": true
    },
    "addresses": {
        "http": "0.0.0.0",
        "dns": "0.0.0.0"
    },
    "ports": {
        "http": 8500,
        "dns": 8600
    },
    "connect": {
        "enabled": true
    },
    "enable_script_checks": true,
    "enable_local_script_checks": true,
    "disable_host_node_id": true
}